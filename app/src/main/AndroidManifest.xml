<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mehome">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Models.Display.Sale.SaleDisplay"></activity>
        <activity android:name=".Models.Display.Rentals.HomeDisplay" />
        <activity android:name=".Models.Display.Holiday.HolidayDisplay" />
        <activity android:name=".Models.AddingProperty.HolidayHouses.HolidayRental" />
        <activity android:name=".Models.AddingProperty.CommercialSale.AddCommercial" />
        <activity android:name=".Models.AddingProperty.SaleHouses.SaleHouse" />
        <activity android:name=".Models.AddingProperty.RentalHouses.HouseRental" />
        <activity android:name=".Models.Display.Commercials.CommercialDisplay" />
        <activity android:name=".Models.Auth.ProfileActivity" />
        <activity
            android:name=".Models.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Models.Auth.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Models.Auth.RegisterActivity" />
        <activity android:name=".Models.AddingProperty.AddProperty" />
        <activity android:name=".Models.Display.MyProperty" />
        <activity android:name=".Models.FindAHome" />
        <activity android:name=".Models.Display.Commercials.Commercial" />
        <activity android:name=".Models.Auth.ResetPasswordActivity" />
    </application>

</manifest>